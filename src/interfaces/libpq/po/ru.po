msgstr "некорректный адрес LDAP \"%s\": должен быть только один атрибут\n"

#: fe-connect.c:3442 fe-connect.c:3499
#, c-format
msgid "invalid LDAP URL \"%s\": must have search scope (base/one/sub)\n"
