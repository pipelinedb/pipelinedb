CREATE VIEW test_gs3 WITH (sw = '5 seconds') AS SELECT x::integer, y::integer, z::integer, COUNT(*) FROM test_gs_stream
	GROUP BY GROUPING SETS (x, y, z);
SELECT pipelinedb.get_worker_querydef('test_gs3');
                                      get_worker_querydef                                       
------------------------------------------------------------------------------------------------
  SELECT date_round(arrival_timestamp, '@ 1 sec'::interval) AS arrival_timestamp,              +
     x,                                                                                        +
     y,                                                                                        +
     z,                                                                                        +
     count(*) AS count                                                                         +
    FROM test_gs_stream                                                                        +
   WHERE (arrival_timestamp > (clock_timestamp() - '@ 5 secs'::interval))                      +
   GROUP BY GROUPING SETS ((x), (y), (z)), (date_round(arrival_timestamp, '@ 1 sec'::interval))
(1 row)

SELECT pipelinedb.get_combiner_querydef('test_gs3');
         get_combiner_querydef         
---------------------------------------
  SELECT arrival_timestamp,           +
     x,                               +
     y,                               +
     z,                               +
     combine(count) AS count          +
    FROM test_gs3_mrel                +
   GROUP BY x, y, z, arrival_timestamp
(1 row)

SELECT pg_get_viewdef('test_gs3');
                                     pg_get_viewdef                                     
----------------------------------------------------------------------------------------
  SELECT test_gs3_mrel.x,                                                              +
     test_gs3_mrel.y,                                                                  +
     test_gs3_mrel.z,                                                                  +
     sw_combine(test_gs3_mrel.count) AS count                                          +
    FROM ONLY test_gs3_mrel                                                            +
   WHERE (test_gs3_mrel.arrival_timestamp > (clock_timestamp() - '@ 5 secs'::interval))+
   GROUP BY test_gs3_mrel.x, test_gs3_mrel.y, test_gs3_mrel.z;
(1 row)

