CREATE FOREIGN TABLE cont_dist_agg_stream (x integer) SERVER pipelinedb;
CREATE VIEW test_dist_agg0 AS
  SELECT x % 10 AS g, dist_agg(x)
 FROM cont_dist_agg_stream
GROUP BY g;
CREATE VIEW test_dist_agg1 AS
  SELECT x % 10 AS g, dist_agg(x)
 FROM cont_dist_agg_stream
GROUP BY g;
