         ->  HashAggregate
               ->  Append
                     ->  Result
                     ->  Result
(8 rows)

SELECT * FROM
  (SELECT 1 AS t, generate_series(1,10) AS x
   UNION
   SELECT 2 AS t, 4 AS x) ss
WHERE x < 4
ORDER BY x;
