-- Test proper handling of parameterized appendrel paths when the
-- potential join qual is expensive
create function expensivefunc(int) returns int
language plpgsql immutable strict cost 10000
as $$begin return $1; end$$;
create temp table t3 as select generate_series(-1000,1000) as x;
create index t3i on t3 (expensivefunc(x));
analyze t3;
explain (costs off, num_nodes off, nodes off)
select * from
  (select * from t3 a union all select * from t3 b) ss
  join int4_tbl on f1 = expensivefunc(x);
                           QUERY PLAN                            
-----------------------------------------------------------------
 Hash Join
   Hash Cond: (int4_tbl.f1 = expensivefunc(a.x))
   ->  Data Node Scan on int4_tbl "_REMOTE_TABLE_QUERY_"
   ->  Hash
         ->  Append
               ->  Data Node Scan on t3 "_REMOTE_TABLE_QUERY__1"
               ->  Data Node Scan on t3 "_REMOTE_TABLE_QUERY__2"
(7 rows)

select * from
  (select * from t3 a union all select * from t3 b) ss
  join int4_tbl on f1 = expensivefunc(x);
 x | f1 
---+----
 0 |  0
 0 |  0
(2 rows)

drop table t3;
drop function expensivefunc(int);
