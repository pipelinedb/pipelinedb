CREATE FOREIGN TABLE sw_vacuum_stream (key text) SERVER pipelinedb;
CREATE VIEW sw_vacuum AS SELECT key, COUNT(*) FROM sw_vacuum_stream WHERE arrival_timestamp > clock_timestamp() - interval '3 second' GROUP BY key;
