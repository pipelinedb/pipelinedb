-- check processing of postponed quals (bug #9041)
explain (verbose, costs off)
select * from
  (select 1 as x) x cross join (select 2 as y) y
  left join lateral (
    select * from (select 3 as z) z where z.z = x.x
  ) zz on zz.z = y.y;
