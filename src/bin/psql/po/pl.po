msgid "  \\z      [PATTERN]      same as \\dp\n"
msgstr "  \\z      [WZORZEC]      to samo co \\dp\n"

#: help.c:229
#, c-format
msgid "Formatting\n"
msgstr "Formatowanie\n"

#: help.c:230
#, c-format
