msgstr "  \\des[+] [WZORZEC]      listuje serwery obce\n"

#: help.c:199
#, c-format
msgid "  \\deu[+] [PATTERN]      list user mappings\n"
msgstr "  \\deu[+] [WZORZEC]      listuje mapowania użytkownika\n"

#: help.c:200
#, c-format
