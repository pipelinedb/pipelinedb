#, c-format
msgid ""
"\n"
"Connection options:\n"
msgstr ""
"\n"
"Options de connexion :\n"

#: pg_basebackup.c:129 pg_receivexlog.c:62
#, c-format
#| msgid "  -d, --dbname=CONNSTR     connect using connection string\n"
