msgstr "  -i, --interactive         删除任何东西之前给予提示\n"

#: droplang.c:203
#, c-format
msgid "%s: language \"%s\" is not installed in database \"%s\"\n"
msgstr "%1$s: 数据库 \"%3$s\" 中, 没有安装语言 \"%2$s\"\n"

