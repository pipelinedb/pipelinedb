#, c-format
msgid ""
"  -c, --connection-limit=N  connection limit for role (default: no limit)\n"
msgstr ""
"  -c, --connection-limit=N       nombre maximal de connexions pour le rôle\n"
"                                 (par défaut sans limite)\n"

